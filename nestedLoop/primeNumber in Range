// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
        int range;
        cout<<"enter any range : ";
        cin>>range;
    for(int i=1; i<=range; i++){
        int count = 0;
        for(int j=1; j<=i; j++){
            if(i%j == 0){
                count++;
            }
        }
        if(count==2){
            cout<<i<<" is prime"<<endl;
        }
        else{
            cout<<i<<" is not prime"<<endl;
        }
    }   
}
